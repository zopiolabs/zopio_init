{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "words": [
    "zopio",
    "zopiolabs",
    "fintech",
    "monorepo",
    "nextjs",
    "gitflow",
    "kubectl",
    "kubernetes",
    "changelog",
    "Dependabot",
    "CODEOWNERS",
    "SARIF",
    "CodeQL",
    "webhook",
    "webhooks",
    "mergeability",
    "rebase",
    "rebased",
    "automerge",
    "autofix",
    "autofixable",
    "cherrypick",
    "cherrypicked",
    "commitlint",
    "commitizen",
    "lockfile",
    "pnpm",
    "vitest",
    "biome",
    "ultracite",
    "APIDOC",
    "SBOM",
    "SPDX",
    "OIDC",
    "allowfullscreen",
    "bgcolor",
    "focusring",
    "fullscreen",
    "novalidate",
    "readonly",
    "srcset",
    "viewports",
    "zoomable",
    "mdx",
    "hotfix",
    "hotfixes",
    "adminzopio",
    "uozopio",
    "umutcanoner",
    "semver",
    "repos",
    "repo's",
    "devops",
    "devsecops"
  ],
  "flagWords": [],
  "ignorePaths": [
    "node_modules/**",
    "pnpm-lock.yaml",
    "package-lock.json",
    "yarn.lock",
    ".git/**",
    ".github/workflows/**",
    "**/*.min.js",
    "**/*.min.css",
    "coverage/**",
    "dist/**",
    "build/**",
    ".next/**",
    "out/**",
    ".turbo/**",
    ".vercel/**",
    "*.log"
  ],
  "ignoreRegExpList": [
    "/\\b[A-Z]{2,}\\b/g",
    "/\\b\\d+\\b/g",
    "/[0-9a-f]{7,40}/gi",
    "/\\b(?:https?|ftp):\\/\\/[^\\s]+/gi",
    "/<!--[\\s\\S]*?-->/g"
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./.github/project-words.txt",
      "addWords": true
    }
  ],
  "dictionaries": [
    "project-words",
    "en-us",
    "typescript",
    "node",
    "npm",
    "html",
    "css",
    "bash",
    "markdown"
  ],
  "languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": [
        "/^\\s*```[\\s\\S]*?^\\s*```/gm",
        "/\\[([^\\]]+)\\]\\([^\\)]+\\)/g"
      ]
    }
  ]
}